{
  "name": "@windblade/unocss-preset",
  "version": "2.0.0-beta.4",
  "description": "Tailwind-inspired UnoCSS preset with considerable improvements",
  "author": "Star Lederer <germans.lederers@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/StarLederer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StarLederer/windblade.git"
  },
  "bugs": {
    "url": "https://github.com/StarLederer/windblade/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "tailwind",
    "windblade"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.css"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@unocss/core": "0.53.3",
    "@unocss/preset-mini": "0.53.3",
    "@windblade/core": "workspace:*",
    "@windblade/unocss-preset-color": "workspace:*",
    "@windblade/unocss-preset-dollars": "workspace:*",
    "ts-deepmerge": "^6.1.0",
    "ts-extras": "^0.11.0",
    "unocss-docs": "workspace:*"
  }
}
